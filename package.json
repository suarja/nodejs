{
  "name": "Editia Primary Server",
  "version": "1.0.0",
  "description": "A Node.js web application with TypeScript for video generation API.",
  "main": "dist/app.js",
  "author": "alphasec",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.825.0",
    "@aws-sdk/s3-request-presigner": "^3.825.0",
    "@clerk/backend": "^2.0.0",
    "@elevenlabs/elevenlabs-js": "^2.2.0",
    "@google/genai": "^1.8.0",
    "@logtail/node": "^0.5.5",
    "@logtail/winston": "^0.5.5",
    "@openai/agents": "^0.0.14",
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "editia-core": "^2.0.6",
    "express": "^4.19.2",
    "multer": "^2.0.1",
    "openai": "^5.1.1",
    "winston": "^3.17.0",
    "zod": "<=3.25.67"
  },
  "devDependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "start": "node dist/app.js",
    "dev": "npm run update-core && npm run build && node dist/app.js",
    "build": "tsc",
    "dev:watch": "nodemon --watch src --ext ts --exec 'npm run build && node dist/app.js'",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "cp -r views public dist/ || true",
    "test": "vitest",
    "test:run": "vitest run --reporter=basic",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:jest": "jest",
    "update-core": "npm install editia-core@latest",
    "test:video": "ts-node src/scripts/test-video-analysis.ts",
    "test:prompt": "jest src/services/__tests__/promptService.test.ts",
    "test:integration": "jest tests/services/creatomateBuilder.test.ts",
    "test:e2e": "jest tests/services/creatomateBuilder.e2e.test.ts"
  }
}
